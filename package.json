{
  "name": "@angularfy/autonumeric",
  "version": "3.0.0",
  "scripts": {
    "ng": "ng",
    "build:demo": "webpack",
    "build:dist": "ng-packagr -p package.json",
    "build:clean": "del-cli dist",
    "commit": "git-cz",
    "copyfiles": "copyfiles CHANGELOG.md dist",
    "prerelease": "npm test",
    "pack": "cd dist && npm pack",
    "release:git": "standard-version && git push --follow-tags origin master",
    "release:npm": "npm run build:dist && npm run copyfiles && npm publish dist",
    "release:pack": "npm run build:dist && npm run copyfiles && npm run pack",
    "release": "npm run release:git && npm run release:npm",
    "postrelease": "npm run build:clean && npm run gh-pages",
    "codecov": "cat coverage/lcov.info | codecov",

  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts"
    }
  },
  "repository": {
    "url": "git://github.com/angularfy/ng-autonumeric.git"
  },
  "peerDependencies": {
    "@angular/core": ">=5.0.0",
    "@angular/forms": ">=5.0.0",
    "autonumeric": "^4.0.1"
  },
  "devDependencies": {
    "@angular/core": ">=5.0.0",
    "@angular/forms": ">=5.0.0",
    "autonumeric": "^4.0.1",
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@angular/compiler": "7.2.15",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "del-cli": "^1.1.0",
    "copyfiles": "^2.0.0",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.34.2",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
